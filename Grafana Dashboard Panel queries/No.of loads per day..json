            "rawSql": "SELECT\r\n    project_name,\r\n    ticket_status,\r\n\r\n    -- First and last ticket dates for this project & status\r\n    MIN(ticket_date::date) AS first_ticket_date,\r\n    MAX(ticket_date::date) AS last_ticket_date,\r\n\r\n    -- Today\r\n    SUM(CASE WHEN ticket_date::date = CURRENT_DATE\r\n        THEN COALESCE(completed_trip_count,0) ELSE 0 END) AS today_loads,\r\n    -- Today hours in HH:MM\r\n    LPAD(FLOOR(SUM(CASE WHEN ticket_date::date = CURRENT_DATE\r\n        THEN COALESCE(total_time_minutes,0) ELSE 0 END) / 60)::text, 2, '0')\r\n    || ':' ||\r\n    LPAD((SUM(CASE WHEN ticket_date::date = CURRENT_DATE\r\n        THEN COALESCE(total_time_minutes,0) ELSE 0 END)::int % 60)::text, 2, '0')\r\n    AS today_hours,\r\n\r\n    -- Last 7 Days (including today)\r\n    SUM(CASE WHEN ticket_date::date >= CURRENT_DATE - INTERVAL '6 days'\r\n        THEN COALESCE(completed_trip_count,0) ELSE 0 END) AS week_loads,\r\n    -- Week hours in HH:MM\r\n    LPAD(FLOOR(SUM(CASE WHEN ticket_date::date >= CURRENT_DATE - INTERVAL '6 days'\r\n        THEN COALESCE(total_time_minutes,0) ELSE 0 END) / 60)::text, 2, '0')\r\n    || ':' ||\r\n    LPAD((SUM(CASE WHEN ticket_date::date >= CURRENT_DATE - INTERVAL '6 days'\r\n        THEN COALESCE(total_time_minutes,0) ELSE 0 END)::int % 60)::text, 2, '0')\r\n    AS week_hours,\r\n\r\n    -- This Month\r\n    SUM(CASE WHEN DATE_TRUNC('month', ticket_date) = DATE_TRUNC('month', CURRENT_DATE)\r\n        THEN COALESCE(completed_trip_count,0) ELSE 0 END) AS month_loads,\r\n    -- Month hours in HH:MM\r\n    LPAD(FLOOR(SUM(CASE WHEN DATE_TRUNC('month', ticket_date) = DATE_TRUNC('month', CURRENT_DATE)\r\n        THEN COALESCE(total_time_minutes,0) ELSE 0 END) / 60)::text, 2, '0')\r\n    || ':' ||\r\n    LPAD((SUM(CASE WHEN DATE_TRUNC('month', ticket_date) = DATE_TRUNC('month', CURRENT_DATE)\r\n        THEN COALESCE(total_time_minutes,0) ELSE 0 END)::int % 60)::text, 2, '0')\r\n    AS month_hours\r\n\r\nFROM tickets\r\nWHERE\r\n    completed_trip_count IS NOT NULL\r\n    AND project_name ILIKE '%' || '${project_name}' || '%'  -- Grafana variable filter\r\nGROUP BY project_name, ticket_status\r\nORDER BY project_name, ticket_status;\r\n",
