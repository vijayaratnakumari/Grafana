            },
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT\r\n    TO_CHAR(sub.month_start, 'Mon') AS month,\r\n    sub.project_name,\r\n    SUM(sub.truck_count) AS total_truck_allocation,\r\n    SUM(sub.trucks_allocated) AS total_trucks_allocated\r\nFROM (\r\n    SELECT\r\n        j.project_name,\r\n        tc.count AS truck_count,\r\n        tc.allocated AS trucks_allocated,\r\n        DATE_TRUNC('month', TO_TIMESTAMP(j.createdAt / 1000)) AS month_start\r\n    FROM jobs j\r\n    JOIN job_truck_categories tc\r\n        ON j.job_id = tc.job_id\r\n    WHERE\r\n        TO_TIMESTAMP(j.createdAt / 1000)::date >= DATE_TRUNC('year', CURRENT_DATE)\r\n        AND (\r\n            '$project_name' = 'All'\r\n            OR j.project_name ILIKE '%' || '$project_name' || '%'\r\n        )\r\n) sub\r\nGROUP BY sub.month_start, sub.project_name\r\nORDER BY sub.month_start, sub.project_name;\r\n",
            "refId": "A",
            "sql": {
              "columns": [
                {
                  "parameters": [],
                  "type": "function"
                }
              ],
              "groupBy": [
                {
                  "property": {
                    "type": "string"
                  },
                  "type": "groupBy"
                }
              ],
              "limit": 50
            }
          },
          {
            "dataset": "etl_db",
            "datasource": {
              "type": "grafana-postgresql-datasource",
              "uid": "eezxjf6xroum8b"
            },
            "editorMode": "code",
            "format": "table",
            "hide": false,
            "rawQuery": true,
            "rawSql": "SELECT\r\n    to_char(to_timestamp(j.createdAt / 1000), 'YYYY-MM-DD') AS day,\r\n    j.project_name,\r\n    SUM(tc.count) AS total_truck_allocation,\r\n    SUM(tc.allocated) AS total_trucks_allocated\r\nFROM jobs j\r\nJOIN job_truck_categories tc\r\n    ON j.job_id = tc.job_id\r\nWHERE to_timestamp(j.createdAt / 1000)::date >= CURRENT_DATE - INTERVAL '30 days'\r\nGROUP BY day, j.project_name\r\nORDER BY day, j.project_name;\r\n",
            "refId": "B",
