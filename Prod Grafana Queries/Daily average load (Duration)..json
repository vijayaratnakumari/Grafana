      "targets": [
        {
          "dataset": "etl_db",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "eezxjf6xroum8b"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "-- ==============================================================  \r\n--  DAILY COMPLETED LOADS (MONTH-TO-DATE)  \r\n--  Time-series friendly output for Grafana  \r\n--  Author: Bujji & Vijaya üåü  \r\n-- ==============================================================  \r\n\r\nWITH latest_date AS (\r\n    SELECT LEAST(MAX(ticket_date::date), CURRENT_DATE) AS last_date\r\n    FROM tickets\r\n    WHERE project_name ILIKE '%' || '${project_name}' || '%'  -- ‚úÖ use Grafana variable\r\n),\r\ndays AS (\r\n    -- Generate one row per calendar day from month start to today\r\n    SELECT generate_series(DATE_TRUNC('month', last_date)::date, last_date::date, '1 day')::date AS day_date\r\n    FROM latest_date\r\n),\r\ndaily_agg AS (\r\n    -- Aggregate tickets per day and status\r\n    SELECT\r\n        ticket_date::date AS day_date,\r\n        project_name,                  -- ‚úÖ show actual project name\r\n        ticket_status,\r\n        SUM(completed_trip_count) AS total_trips,\r\n        SUM(total_time_minutes) AS total_time_minutes\r\n    FROM tickets\r\n    WHERE\r\n        ticket_status NOT IN ('NOT_STARTED', 'CANCELLED')\r\n        AND project_name ILIKE '%' || '${project_name}' || '%'  -- ‚úÖ filter dynamically\r\n    GROUP BY ticket_date::date, project_name, ticket_status\r\n)\r\nSELECT\r\n    d.day_date::timestamp AS \"time\",   -- Grafana time column\r\n    TO_CHAR(d.day_date, 'Dy, DD Mon YYYY') AS day_label,\r\n    a.project_name,                    -- ‚úÖ actual project name\r\n    a.ticket_status,\r\n    COALESCE(a.total_trips, 0) AS total_trips,\r\n\r\n    -- Total hours in HH:MM format\r\n    CASE \r\n        WHEN a.total_time_minutes IS NULL THEN '00:00'\r\n        ELSE LPAD(FLOOR(a.total_time_minutes / 60)::text, 2, '0') \r\n             || ':' || LPAD((a.total_time_minutes::int % 60)::text, 2, '0')\r\n    END AS total_hours,\r\n\r\n    -- Average duration per trip in HH:MM\r\n    CASE \r\n        WHEN a.total_trips IS NULL OR a.total_trips = 0 THEN '00:00'\r\n        ELSE LPAD(FLOOR((a.total_time_minutes::float / a.total_trips) / 60)::text, 2, '0')\r\n             || ':' || LPAD((ROUND(a.total_time_minutes::float / a.total_trips)::int % 60)::text, 2, '0')\r\n    END AS avg_duration_per_trip\r\n\r\nFROM days d\r\nLEFT JOIN daily_agg a ON a.day_date = d.day_date\r\nORDER BY d.day_date ASC;\r\n",
          "refId": "A",
          "sql": {
