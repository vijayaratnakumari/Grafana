          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n    ticket_date::date AS day,\r\n    SUM(completed_trip_count) AS total_completed_trips,\r\n    -- Total duration converted to HH:MM\r\n    COALESCE(\r\n        TO_CHAR(\r\n            INTERVAL '1 minute' * SUM(total_time_minutes),\r\n            'HH24:MI'\r\n        ),\r\n        '00:00'\r\n    ) AS total_hours,\r\n    -- Average duration per trip in HH:MM\r\n    COALESCE(\r\n        TO_CHAR(\r\n            INTERVAL '1 minute' * (SUM(total_time_minutes)::float / NULLIF(SUM(completed_trip_count), 0)),\r\n            'HH24:MI'\r\n        ),\r\n        '00:00'\r\n    ) AS avg_duration_per_trip\r\nFROM tickets\r\nWHERE completed_trip_count IS NOT NULL\r\n  AND ticket_date::date = CURRENT_DATE\r\nGROUP BY day\r\nORDER BY day;\r\n",
          "refId": "A",
