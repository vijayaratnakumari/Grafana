            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT\r\n    driver_name,\r\n    DATE_TRUNC('week', ticket_date) AS week_start,\r\n    COUNT(*) AS total_loads,\r\n    EXTRACT(EPOCH FROM (MAX(end_date) - MIN(start_date))) / 60.0 / COUNT(*) AS avg_load_duration_minutes\r\nFROM tickets\r\nWHERE ticket_status IN ('LOADED_IN_PROGRESS', 'inprogress')\r\nGROUP BY driver_name, week_start\r\nORDER BY week_start, driver_name;\r\n",
